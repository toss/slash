{
  "name": "@toss/storage",
  "version": "1.4.0",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./typed": "./src/typed/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "files": [
    "dist",
    "esm",
    "typed.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/toss/slash.git",
    "directory": "packages/common/storage"
  },
  "scripts": {
    "build": "tsc --emitDeclarationOnly --outDir types && rollup -c && rm -rf types",
    "prepack": "yarn build",
    "test": "jest",
    "coverage": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@toss/jest": "workspace:^",
    "@types/babel__core": "^7",
    "@types/jest": "^28.1.8",
    "@types/node": "^14.14.41",
    "babel-jest": "^29",
    "concurrently": "^4.1.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "import": {
          "types": "./esm/index.d.mts",
          "default": "./esm/index.mjs"
        }
      },
      "./typed": {
        "require": {
          "types": "./dist/typed/index.d.ts",
          "default": "./dist/typed/index.js"
        },
        "import": {
          "types": "./esm/typed/index.d.mts",
          "default": "./esm/typed/index.mjs"
        }
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "module": "./esm/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "gitHead": "6a4434621e29e014d537ae3dc0f026364b893a69"
}
