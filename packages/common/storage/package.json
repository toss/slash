{
  "name": "@toss/storage",
  "version": "1.2.0",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./typed": "./src/typed/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "rm -rf dist esm typed && tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --declarationDir dist && rollup -c",
    "prepack": "yarn build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@toss/rollup-config": "workspace:^0.1.4",
    "@tossteam/jest": "^17",
    "@types/babel__core": "^7",
    "@types/jest": "^28.1.8",
    "@types/node": "^14.14.41",
    "babel-jest": "^29",
    "concurrently": "^4.1.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "typescript": "4.8.3"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "./typed": {
        "require": "./typed/dist/index.js",
        "import": "./typed/esm/index.mjs",
        "types": "./typed/dist/index.d.ts"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "module": "./esm/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "gitHead": "6a4434621e29e014d537ae3dc0f026364b893a69"
}
